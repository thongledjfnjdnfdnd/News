{
  "name": "practice-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd .. && husky install practice-nextjs/.husky",
    "pre-commit-lint": "yarn lint-staged",
    "start-db": "json-server --watch json-server/db.json --port 4000",
    "run-app": "concurrently \"yarn start-db\" \"yarn dev\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:bail": "jest --bail",
    "test:updateSnapshots": "jest -u"
  },
  "dependencies": {
    "concurrently": "^7.4.0",
    "jest-fetch-mock": "^3.0.3",
    "json-server": "^0.17.0",
    "next": "12.3.0",
    "next-router-mock": "^0.7.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-actions": "^6.5.11",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-interactions": "^6.5.11",
    "@storybook/addon-links": "^6.5.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.11",
    "@storybook/manager-webpack4": "^6.5.11",
    "@storybook/react": "^6.5.11",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/node": "18.7.17",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.9",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.8.3"
  }
}
